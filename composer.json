{
  "name": "tyrads/tyrads-sdk",
  "description": "Official TyrAds SDK for PHP",
  "version": "0.1.3",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "TyrAds Tech",
      "email": "tech@tyrads.com",
      "homepage": "https://tyrads.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tyrads\\TyradsSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^5.0|^6.0|^7.0"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.3|^2.0",
    "squizlabs/php_codesniffer": "^3.6",
    "pestphp/pest": "^1.22"
  },
  "scripts": {
    "lint": "phpcs",
    "lint:fix": "phpcbf",
    "test": "pest",
    "test:coverage": "pest --coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
