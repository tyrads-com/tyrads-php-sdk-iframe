{
  "name": "tyrads/tyrads-sdk",
  "description": "Official TyrAds SDK for PHP - Compatible with PHP 5.5+ through PHP 8.4",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "tyrads",
    "sdk",
    "api",
    "offerwall",
    "iframe",
    "php5",
    "php7",
    "php8"
  ],
  "homepage": "https://tyrads.com",
  "readme": "README.md",
  "support": {
    "issues": "https://github.com/tyrads-com/tyrads-php-sdk-iframe/issues",
    "source": "https://github.com/tyrads-com/tyrads-php-sdk-iframe",
    "docs": "https://github.com/tyrads-com/tyrads-php-sdk-iframe/blob/main/README.md"
  },
  "authors": [
    {
      "name": "TyrAds Tech",
      "email": "tech@tyrads.com",
      "homepage": "https://tyrads.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tyrads\\TyradsSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^5.3|^6.0|^7.0"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.3",
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^4.8|^5.7|^6.5|^7.5|^8.5|^9.6|^10.0|^11.0"
  },
  "scripts": {
    "lint": "phpcs",
    "lint:fix": "phpcbf",
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-text"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
